{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "movie-library/persons-block",
	"title": "People",
	"category": "movie-library",
	"icon": "businessperson",
	"description": "Displays a list of person posts.",
	"keywords": ["mlib-person", "people", "list", "persons"],
	"version": "0.1.0",
	"textdomain": "movie-library",
	"supports": {
		"html": false
	},
	"attributes": {
		"count": {
			"type": "integer",
			"default": 5
		},
		"career": {
			"type": "integer",
			"default": 0
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
